{
  "name": "local-config-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 5173",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "dev:no-cache": "vite dev --port 5173 --force --clearScreen false",
    "no-cache": "./no-cache-dev.sh",
    "sync-docs": "node scripts/sync-docs.js",
    "generate-manifest": "node scripts/generate-docs-manifest.js",
    "prepare-docs": "npm run sync-docs && npm run generate-manifest",
    "prebuild": "npm run prepare-docs",
    "build": "vite build",
    "preview": "vite preview --port 5173"
  },
  "dependencies": {
    "@radix-ui/react-tabs": "^1.1.1",
    "focus-trap-react": "^11.0.4",
    "fuse.js": "^7.1.0",
    "lightningcss": "^1.30.1",
    "lucide-react": "^0.546.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.26.2",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^4.0.15",
    "jsdom": "^27.3.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vitest": "^4.0.15"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-arm64": "^4.52.4"
  }
}
